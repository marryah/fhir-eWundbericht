<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <url value="https://posithera.de/fhir/eWundbericht/StructureDefinition/VerbalAssessments" />
  <name value="PosiTheraVerbalAssessments" />
  <status value="draft" />
  <description value="Profile for everything within the wound summary which is not wound-specific, but just a verbal description, e.g. the hygiene assessment, social factors, adherence and acceptance of the therapy. A further distinction of the content is given by the code." />
  <publisher value="Hochschule Osnabrück" />
  <contact>
    <name value="Mareike Przysucha" />
    <telecom>
      <system value="email" />
      <value value="ewundbericht@hs-osnabrueck.de" />
      <use value="work" />
    </telecom>
    <telecom>
      <system value="phone" />
      <value value="+49 541 969 3354" />
      <use value="work" />
    </telecom>
  </contact>
  <copyright value="Forschungsgruppe Informatik im Gesundheitswesen, Fakultät Wirtschafts- und Sozialwissenschaften, Hochschule Osnabrück" />
  <fhirVersion value="4.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Observation" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Observation" />
  <derivation value="constraint" />
  <differential>
    <element id="Observation.code">
      <path value="Observation.code" />
      <binding>
        <strength value="extensible" />
        <valueSet value="https://posithera.de/fhir/eWundbericht/ValueSet/ContentVerbalAssessment" />
      </binding>
    </element>
    <element id="Observation.subject">
      <path value="Observation.subject" />
      <min value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="https://posithera.de/fhir/eWundbericht/StructureDefinition/Patient" />
      </type>
    </element>
    <element id="Observation.value[x]">
      <path value="Observation.value[x]" />
      <type>
        <code value="string" />
      </type>
      <mustSupport value="false" />
    </element>
    <element id="Observation.interpretation">
      <path value="Observation.interpretation" />
      <max value="0" />
    </element>
    <element id="Observation.component">
      <path value="Observation.component" />
      <max value="0" />
    </element>
  </differential>
</StructureDefinition>